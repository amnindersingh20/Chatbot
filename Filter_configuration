from docx import Document

# Load your Word doc
doc = Document("YourSpecDoc.docx")

# Extract all text from the document
all_text = []
for para in doc.paragraphs:
    text = para.text.strip()
    if text:  # ignore empty lines
        all_text.append(text)

# Print raw values with repr to see hidden chars
print("=== Fields from Word ===")
for t in all_text:
    print(f"Raw: {t} | repr: {repr(t)} | length: {len(t)}")
